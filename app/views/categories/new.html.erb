<h1>New category :</h1>


<%= semantic_form_for @category do |f| %>
    <%= f.inputs do %>
       <li> <%= f.label :name %>
            <%= f.input :name %> </li>

    <% end %>
    <%= f.actions do %>
        <%= f.action :submit, :as => :button %>
        <%= f.action :cancel, :as => :link %>
    <% end %>
<% end %>
